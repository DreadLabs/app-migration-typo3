{
  "name": "dreadlabs/app-migration-typo3",
  "description": "Integrates dreadlabs/app-migration infrastructure into TYPO3.CMS.",
  "type": "typo3-cms-extension",
  "minimum-stability": "alpha",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas Juhnke",
      "email": "typo3@van-tomas.de"
    }
  ],
  "require": {
    "dreadlabs/app-migration-lock-ninjamutex": "~0.1.0",
    "dreadlabs/app-migration-migrator-phinx": "~0.1.0"
  },
  "require-dev": {
    "typo3/cms": "7.4.0",
    "phploc/phploc": "~2.1",
    "squizlabs/php_codesniffer": "~2.3",
    "sebastian/phpcpd": "~2.0",
    "phpunit/phpunit": "~4.6",
    "pdepend/pdepend": "~2.0",
    "phpmd/phpmd": "~2.2"
  },
  "replace": {
    "typo3/cms-composer-installers": "*"
  },
  "autoload": {
    "psr-4": {
      "DreadLabs\\AppMigrationTypo3\\": "Classes/",
      "DreadLabs\\AppMigrationTypo3\\Tests\\": "tests/"
    }
  }
}
